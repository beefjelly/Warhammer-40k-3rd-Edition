          <constraints>
            <constraint field="selections" scope="parent" value="1.0" percentValue="false" shared="true" includeChildSelections="false" includeChildForces="false" id="2342-b604-1b17-e811" type="max"/>
          </constraints>
          <rules>
            <rule id="4204-a18e-b754-bdf6" name="Illum Flare" hidden="false">
              <description>This flare is used in missions where the Night Fighting rules are in effect. They allow one enemy unit spotted by the aircraft to be fired at in the next turn by any other ground forces in range and that have line of sight.</description>
            </rule>
          </rules>
